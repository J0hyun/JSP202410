    pageEncoding="UTF-8"%>

<%@ taglib prefix="c" uri="jakarta.tags.core" %>

<html>
<head><title>JSTL - forEach 1</title></head>
<body>
	<h3>일반 for문 형태의 forEach 태그</h3>
	<c:forEach begin="1" end="10" step="2" var="i">
	<p>반복 ${i} </p>
	</c:forEach>
